+					}
+				}
+			}
+			if(adr != 0x00){break;}
+		}
+	}
+	else if(type == 'r' || type == 'R'){
+		for(int i = length-1 ; i>=0 ; --i){
+			if(str[i] == text[t_length-1]){
+				for(unsigned int j = 0 ; (j<t_length) && (i-j>=0) ; ++j){
+					if(str[i-j] != text[(t_length-1)-j]){break;}
+					if(j == (t_length-1)){
+						adr = (str+(i-j));
+						break;
+					}
+				}
+			}
+			if(adr != 0x00){break;}
+		}
+	}
+
+	return adr;
